<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="399.0" prefWidth="232.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.EditEmployeeViewController">
    <children>
        <Label layoutX="37.0" layoutY="14.0" text="EDIT EMPLOYEE">
            <font>
                <Font name="System Bold" size="21.0" />
            </font>
        </Label>
        <Label fx:id="nameEmployee" layoutX="58.0" layoutY="69.0" text="Employee Name">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <TextField fx:id="txtAge" layoutX="41.0" layoutY="131.0" promptText="ex: 35" />
        <Label layoutX="41.0" layoutY="107.0" text="Age" />
      <TextField fx:id="txtRole" layoutX="41.0" layoutY="198.0" promptText="ex: Intern" />
      <Label layoutX="41.0" layoutY="174.0" text="Role" />
      <Label layoutX="41.0" layoutY="247.0" text="Department" />
      <Button fx:id="btnClose" layoutX="26.0" layoutY="340.0" mnemonicParsing="false" onAction="#onBtnClose" text="Close" />
      <Button fx:id="btnUpdate" layoutX="129.0" layoutY="340.0" mnemonicParsing="false" onAction="#onBtnUpdate" text="Update" />
      <ComboBox fx:id="cbDepartment" layoutX="41.0" layoutY="272.0" prefWidth="150.0" />
    </children>
</AnchorPane>
